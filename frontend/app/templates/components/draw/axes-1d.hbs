<div>axesP : {{axesP.length}}</div>
{{#each axesP as |axis axisIndex|}}
  {{!-- axis is the primary / reference block of the axis --}}
  {{#draw/axis-1d drawMap=drawMap axis=axis axes2d=axes2d as |axis1d|}}
    <div>axis : {{axis1d.axis.id}}, {{axis1d.extended}}</div>
    {{#if axis1d.extended}}
      {{axis-2d drawMap=drawMap data=drawMap.oa axis1d=axis1d axisID=axis1d.axis.id axisObj=(hash axisID=axis1d.axis.id extended=axis1d.extended)
        qtlBlocks=axis1d.qtlBlocks }}
    {{/if}}

    {{draw/axis-ticks-selected axis1d=axis1d axisId=axis.id
      drawMap=drawMap
      featuresInBlocks=featuresInBlocks
    }}
  {{/draw/axis-1d}}
{{/each}}
